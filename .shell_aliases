alias dit='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'

alias :q='exit'
alias ra='ranger'
alias vs='code'
alias v.='vim .'
alias chrome='google-chrome'

alias m='make'
alias mc='make clean'

# tmux aliases
